{% extends 'OdiseoConAgraBundle:Frontend:layout.html.twig' %}
 
{% block content %}
<div class="quien_es marcaConsumo">
	<div class="mainTitleContent clearfix">
		<h1 class="mainTitle pull-left"></h1>
		<a href="{{ path('odiseo_conagra_frontend_quien_es_page', { 'page': 'clases-de-marcas' }) }}" class="backButton pull-right">
			{{ 'back_to_home' | trans | raw }}
		</a>
	</div>
	<div class="customDiv_foreground"> 
		<div>
			 <p class="mainTitle"> {{ 'quien_es_consumidor' | trans | raw }} </p>
			 <p class="text">
			 	{{ 'quien_es_consumidor_description' | trans | raw }} 
			 </p>
			 <ul>{{ 'quien_es_consumidor_li' | trans | raw }}
			 </ul>
			 <span class="asterisk">{{ 'quien_es_consumidor_asterisk' | trans | raw }}</span>
		</div>
		<div class="customDiv_green_1">
		</div> 
		<div class="customDiv_green_2">
		</div>
		<img src="{{ asset('bundles/odiseoconagra/images/quienesconagra/consumidor.png') }}" alt="consumidor"  class="img-responsive products">
	</div>
	<a href="{{ path('odiseo_conagra_frontend_quien_es_page', { 'page': 'marca-de-consumo' }) }}" class="pageButton previousButton"
		data-toggle="popover" data-content="{{'quien_es_marca_consumo_page' | trans | raw }}" data-placement="top" title="1/3" data-trigger="hover">
	</a>
	<a href="{{ path('odiseo_conagra_frontend_quien_es_page', { 'page': 'many-stories' }) }}" class="pageButton nextButton" 
		data-toggle="popover" data-content="{{'quien_es_marca_consumo_page' | trans | raw }}" data-placement="top" title="3/3" data-trigger="hover">
	</a>
</div>
{% endblock %}
