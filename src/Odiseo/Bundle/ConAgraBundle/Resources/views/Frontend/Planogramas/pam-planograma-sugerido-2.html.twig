{% extends 'OdiseoConAgraBundle:Frontend:layout.html.twig' %}
 
{% block content %}
<div class="planogramas pam">
	<div class="title clearfix">
		<h1 class="mainTitle">Planograma Sugerido</h1>
		<a href="{{ path('odiseo_conagra_frontend_planogramas_index') }}" class="backButton pull-right">
			<img src="{{ asset('bundles/odiseoconagra/images/volverInicioButton.png') }}">
		</a>
	</div>
	<div class="row">
		<a href="{{ path('odiseo_conagra_frontend_planogramas_page', { 'page': 'pam-planograma-sugerido-3' }) }}">
			<div class="img1 col-md-6 col-sm-6 col-xs-12">
				<img src="{{ asset('bundles/odiseoconagra/images/planogramas/pam/pam_2_1.png') }}" alt="pam"  class="img-responsive pull-left">
			</div>
			<div class="img2 col-md-6 col-sm-6 col-xs-12">
				<img src="{{ asset('bundles/odiseoconagra/images/planogramas/pam/pam_2_2.png') }}" alt="pam"  class="img-responsive pull-left">
			</div>
		</a>
	</div>
	<a href="{{ path('odiseo_conagra_frontend_planogramas_page', { 'page': 'pam-indicaciones-generales' }) }}" class="pageButton previousButton"
		data-toggle="popover" data-content="{{ 'planograms_suggested' | trans |  raw }}" data-placement="top" title="1/3" data-trigger="hover">
	</a>
	<a href="{{ path('odiseo_conagra_frontend_planogramas_page', { 'page': 'pam-planograma-sugerido-3' }) }}" class="pageButton nextButton" 
		data-toggle="popover" data-content="{{ 'planograms_suggested' | trans |  raw }}" data-placement="top" title="3/3" data-trigger="hover">
	</a>
</div>
{% endblock %}
